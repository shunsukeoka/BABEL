{
    "name": "babel",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "tauri": "tauri",
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "format:check": "prettier --check --ignore-path .gitignore --plugin-search-dir=. ./**/*.{json,css,scss,js,ts,cjs,svelte}",
        "format:fix": "prettier --write --ignore-path .gitignore --plugin-search-dir=. ./**/*.{json,css,scss,js,ts,cjs,svelte}",
        "lint": "eslint --ignore-path .gitignore . --ext .js,.ts,.cjs,.svelte",
        "lint:fix": "eslint --fix --ignore-path .gitignore . --ext .js,.ts,.cjs,.svelte",
        "stylelint": "stylelint --ignore-path .gitignore ./**/*.{css,scss,svelte}",
        "stylelint:fix": "stylelint --fix --ignore-path .gitignore ./**/*.{css,scss,svelte}",
        "fix": "npm-run-all -s format:fix lint:fix stylelint:fix",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "test": "jest src",
        "test:coverage": "npm run test -- --coverage",
        "test:watch": "npm run test -- --watch",
        "prepare": "husky install",
        "lint-staged": "lint-staged",
        "commitlint": "commitlint"
    },
    "lint-staged": {
        "*.{json,css,scss,js,ts,cjs,svelte}": [
            "prettier --write"
        ],
        "*.{js,ts,cjs,svelte}": [
            "eslint --fix"
        ],
        "*.{css,scss,svelte}": [
            "stylelint --fix"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.17.2",
        "@babel/preset-env": "^7.16.11",
        "@commitlint/cli": "^16.2.1",
        "@commitlint/config-conventional": "^16.2.1",
        "@storybook/addon-actions": "^6.5.0-alpha.39",
        "@storybook/addon-essentials": "^6.5.0-alpha.39",
        "@storybook/addon-links": "^6.5.0-alpha.39",
        "@storybook/addon-svelte-csf": "^1.1.0",
        "@storybook/svelte": "^6.5.0-alpha.39",
        "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
        "@tauri-apps/cli": "^1.0.0-rc.5",
        "@testing-library/jest-dom": "^5.16.2",
        "@testing-library/svelte": "^3.0.3",
        "@testing-library/user-event": "^13.5.0",
        "@tsconfig/svelte": "^2.0.1",
        "@types/jest": "^27.4.1",
        "@typescript-eslint/eslint-plugin": "^5.11.0",
        "@typescript-eslint/parser": "^5.11.0",
        "babel-jest": "^27.5.1",
        "babel-loader": "^8.2.3",
        "babel-plugin-transform-vite-meta-env": "^1.0.3",
        "eslint": "^8.8.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jest": "^26.1.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-storybook": "^0.5.6",
        "eslint-plugin-svelte3": "^3.4.1",
        "husky": "^7.0.4",
        "jest": "^27.5.1",
        "jest-transform-stub": "^2.0.0",
        "lint-staged": "^12.3.5",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.2.15",
        "postcss-html": "^1.3.0",
        "prettier": "^2.5.1",
        "prettier-plugin-svelte": "^2.6.0",
        "prettier-stylelint": "^0.4.2",
        "sass": "^1.49.7",
        "storybook-builder-vite": "^0.1.15",
        "stylelint": "^14.5.0",
        "stylelint-config-html": "^1.0.0",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recess-order": "^3.0.0",
        "stylelint-config-standard": "^25.0.0",
        "stylelint-scss": "^4.1.0",
        "svelte": "^3.46.4",
        "svelte-check": "^2.2.7",
        "svelte-jester": "^2.3.2",
        "svelte-loader": "^3.1.2",
        "svelte-preprocess": "^4.10.1",
        "ts-jest": "^27.1.3",
        "tslib": "^2.3.1",
        "typescript": "^4.5.4",
        "vite": "^2.8.0"
    },
    "dependencies": {
        "@tauri-apps/api": "^1.0.0-rc.1",
        "the-new-css-reset": "^1.4.9"
    }
}
