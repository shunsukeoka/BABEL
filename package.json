{
    "name": "babel",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "tauri": "tauri",
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "format:check": "prettier --check --ignore-path .gitignore --plugin-search-dir=. \"**/*.{json,yml,css,scss,js,jsx,ts,tsx,cjs}\"",
        "format:fix": "prettier --write --ignore-path .gitignore --plugin-search-dir=. \"**/*.{json,yml,css,scss,js,jsx,ts,tsx,cjs}\"",
        "lint": "eslint --ignore-path .gitignore \"**/*.{js,jsx,ts,tsx,cjs}\"",
        "lint:fix": "eslint --fix --ignore-path .gitignore \"**/*.{js,jsx,ts,tsx,cjs}\"",
        "stylelint": "stylelint --ignore-path .gitignore \"**/*.{css,scss,jsx,tsx}\"",
        "stylelint:fix": "stylelint --fix --ignore-path .gitignore \"**/*.{css,scss,jsx,tsx}\"",
        "fix": "npm-run-all -s format:fix lint:fix stylelint:fix",
        "test": "jest --config ./jest.config.json",
        "test:coverage": "npm run test -- --coverage",
        "test:watch": "npm run test -- --watch",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "prepare": "husky install",
        "lint-staged": "lint-staged",
        "commitlint": "commitlint"
    },
    "lint-staged": {
        "*.{json,css,scss,js,jsx,ts,tsx,cjs}": [
            "prettier --write"
        ],
        "*.{js,jsx,ts,tsx,cjs}": [
            "eslint --fix"
        ],
        "*.{css,scss,jsx,tsx}": [
            "stylelint --fix"
        ]
    },
    "dependencies": {
        "@spectrum-web-components/bundle": "^0.25.1",
        "@storybook/react": "^6.5.9",
        "@tauri-apps/api": "^1.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router": "^6.3.0",
        "react-router-dom": "^6.3.0",
        "react-window": "^1.8.7",
        "styled-components": "^5.3.5"
    },
    "devDependencies": {
        "@babel/core": "^7.18.5",
        "@commitlint/cli": "^17.0.2",
        "@commitlint/config-conventional": "^17.0.2",
        "@storybook/addon-actions": "^6.5.9",
        "@storybook/addon-essentials": "^6.5.9",
        "@storybook/addon-interactions": "^6.5.9",
        "@storybook/addon-links": "^6.5.9",
        "@storybook/builder-vite": "^0.1.36",
        "@storybook/testing-library": "^0.0.13",
        "@stylelint/postcss-css-in-js": "^0.38.0",
        "@tauri-apps/cli": "^1.0.0",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^12.1.5",
        "@testing-library/react-hooks": "^7.0.2",
        "@testing-library/user-event": "^14.2.1",
        "@types/jest": "^27.5.2",
        "@types/node": "^18.0.0",
        "@types/react": "^18.0.12",
        "@types/react-dom": "^18.0.5",
        "@types/styled-components": "^5.1.25",
        "@types/testing-library__jest-dom": "^5.14.5",
        "@typescript-eslint/eslint-plugin": "^5.29.0",
        "@typescript-eslint/parser": "^5.29.0",
        "@vitejs/plugin-react": "^1.3.2",
        "babel-loader": "^8.2.5",
        "eslint": "^8.18.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^26.5.3",
        "eslint-plugin-jest-dom": "^4.0.2",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-storybook": "^0.5.13",
        "eslint-plugin-testing-library": "^5.5.1",
        "husky": "^8.0.1",
        "jest": "^27.5.1",
        "lint-staged": "^13.0.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "sass": "^1.53.0",
        "stylelint": "^14.9.1",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recess-order": "^3.0.0",
        "stylelint-config-standard": "^26.0.0",
        "stylelint-config-standard-scss": "^4.0.0",
        "stylelint-prettier": "^2.0.0",
        "stylelint-processor-styled-components": "^1.10.0",
        "stylelint-scss": "^4.2.0",
        "ts-jest": "^27.1.5",
        "typescript": "^4.7.4",
        "vite": "^2.9.12",
        "vite-plugin-svgr": "^2.2.0"
    }
}
